{
  "compilerOptions": {
    "module": "commonjs",
    "incremental": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "lib": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "strictNullChecks": false,
    "target": "es6",
    "sourceMap": true,
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "noEmit": false,
    "paths": {
      "@src/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    ".next",
    "static",
    "public",
    "assets",
    "dist"
  ],
  "types": [
    "typePatches",
    "node"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
